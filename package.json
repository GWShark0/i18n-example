{
  "name": "i18n-example",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "build": "webpack --mode=development",
    "dev": "npm run build && node ./bin/www",
    "i18n:generate": "node ./i18n/scripts/extract-intl-messages",
    "i18n:translate": "node ./i18n/scripts/translate",
    "i18n": "run-s i18n:generate i18n:translate",
    "now-build": "npm run build"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@google-cloud/translate": "^3.0.1",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-intl-auto": "^1.7.0",
    "bluebird": "^3.5.4",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "extract-react-intl-messages": "^1.0.1",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "helmet": "^3.18.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-flags-select": "^1.1.10",
    "react-intl": "^2.8.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2"
  }
}
